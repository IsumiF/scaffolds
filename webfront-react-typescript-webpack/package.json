{
  "name": "webfront-react-typescript-webpack",
  "version": "0.1.0",
  "main": "src/index.tsx",
  "description": "Web frontend template for React, TypeScript and webpack",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack-config.json\" webpack-dev-server --open --config webpack.dev.ts",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack-config.json\" webpack --config webpack.prod.ts",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" ts-mocha -p tsconfig.json src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IsumiF/scaffolds.git"
  },
  "author": "Isumi Feng",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/IsumiF/scaffolds/issues"
  },
  "homepage": "https://github.com/IsumiF/scaffolds#readme",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.5",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-server": "^3.1.2",
    "@types/webpack-merge": "^4.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "source-map-loader": "^0.2.4",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}
